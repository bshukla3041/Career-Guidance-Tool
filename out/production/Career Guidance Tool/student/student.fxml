<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #E8FF7F;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.StudentController">
   <children>
      <Label fx:id="userLbl" alignment="TOP_LEFT" layoutX="207.0" layoutY="56.0" text="Welcome Student">
         <font>
            <Font name="Ubuntu" size="23.0" />
         </font>
      </Label>
      <JFXButton fx:id="testBtn" layoutX="166.0" layoutY="174.0" onAction="#handleTest" style="-fx-background-color: #760324;" text="Online Test" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor>
         <font>
            <Font name="Ubuntu" size="17.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="resultBtn" layoutX="336.0" layoutY="174.0" onAction="#handleResult" style="-fx-background-color: #760324;" text="Result Analysis" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor>
         <font>
            <Font name="Ubuntu" size="17.0" />
         </font>
      </JFXButton>
      <Button fx:id="logoutBtn" layoutX="492.0" layoutY="23.0" mnemonicParsing="false" onAction="#handleLogout" text="Logout">
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/logout-button.jpg" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="Ubuntu" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
